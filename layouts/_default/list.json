{
  {{ range $i, $e := .Data.Pages.ByTitle }}
    {{ if $i }}, {{ end }}
    "{{ .RelPermalink }}": {
      "id": "{{ .UniqueID }}",
      "title": "{{ lower .Title }}",
      "date": "{{ .Date }}",
      "url": "{{ .Permalink }}",
      "link": "{{ .RelPermalink }}",
      "link_title": "{{ .LinkTitle }}"
      "parent": "{{ if .Parent }}{{ lower .Parent.Title }}{{ end }}"
    }
  {{ end }}
}
